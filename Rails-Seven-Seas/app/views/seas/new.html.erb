<%= form_for @sea do |f| %>
  Name: <%= f.text_field :name %><br>
  Temperature: <%= f.number_field :temperature %><br>
  Bio: <%= f.text_area :bio %><br>
  Image URL: <%= f.text_field :image_url %><br>
  Mood: <%= f.text_field :mood %><br>
  Favorite Color: <%= f.text_field :favorite_color %><br>
  Scariest Creature: <%= f.text_field :scariest_creature %><br>
  Has Mermaids? <%= f.check_box :has_mermaids %><br>
  <%=  f.submit %>
<% end %>